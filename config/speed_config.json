{
  "download_optimization": {
    "chunk_size_mb": 5,
    "max_connections": 16,
    "buffer_size_mb": 10,
    "timeout_seconds": 30,
    "retry_attempts": 3,
    "resume_enabled": true
  },
  
  "upload_optimization": {
    "chunk_size_mb": 2,
    "parallel_uploads": 5,
    "compression_level": 6,
    "timeout_seconds": 60,
    "resume_enabled": true
  },
  
  "compression": {
    "enabled": true,
    "algorithm": "brotli",
    "level": 6,
    "min_size_mb": 1,
    "extensions": [".txt", ".log", ".json", ".xml", ".html"]
  },
  
  "caching": {
    "enabled": true,
    "memory_cache_mb": 100,
    "disk_cache_gb": 5,
    "ttl_seconds": 3600,
    "cleanup_interval_minutes": 60
  },
  
  "network": {
    "tcp_fast_open": true,
    "tcp_no_delay": true,
    "keep_alive": true,
    "max_retries": 3,
    "connection_pool_size": 20
  },
  
  "performance": {
    "thread_pool_size": 16,
    "io_bound_threads": 8,
    "cpu_bound_threads": 4,
    "max_memory_mb": 512,
    "gc_threshold": [700, 10, 10]
  },
  
  "cdn": {
    "enabled": false,
    "servers": ["cdn1.example.com", "cdn2.example.com"],
    "strategy": "round_robin",
    "cache_ttl": 3600
  }
}
