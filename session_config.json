{
  "max_sessions": 5,
  "session_lifetime_hours": 168,
  "auto_rotate": true,
  "rotate_after_errors": 3,
  "backup_count": 10,
  "encryption_enabled": true,
  "compress_sessions": true,
  "geo_diversity": false,
  "device_rotation": true,
  "rotation_schedule": {
    "daily_rotate": true,
    "rotate_time": "04:00",
    "rotate_on_error": true,
    "rotate_on_flood_wait": true
  },
  "validation": {
    "validate_on_start": true,
    "validate_interval_hours": 6,
    "auto_fix_corrupted": true
  },
  "security": {
    "encryption_key_rotation_days": 30,
    "session_file_permissions": 600,
    "backup_encryption": true,
    "log_sensitive_operations": false
  },
  "performance": {
    "session_cache_size": 3,
    "preload_sessions": false,
    "connection_pooling": true
  }
}
